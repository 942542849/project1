<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="dao.Book_Dao">    <!-- xml与接口dao关联 -->
 <select id="select" resultType="model.Book" parameterType="java.lang.String">
 select book.*,Type.name Typename from book inner join type on book.typeid=type.id ${_parameter}
 </select>
 
 </mapper>